{
  "schemaVersion": "1.5.0",
  "id": "SmokeTest",
  "name": "Smoke Test",
  "description": "Sample smoke test suite",
  "version": "1.0.0",
  "tags": [
    "smoke",
    "sanity"
  ],
  "controls": {
    "repeat": 1,
    "maxParallel": 1,
    "continueOnFailure": false,
    "retryOnError": 0,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "TimezoneCheck",
      "ref": "TimezoneCheck",
      "inputs": {
        "ExpectedUtcOffset": "\u002B08:00"
      }
    },
    {
      "nodeId": "DeviceHealthCheck",
      "ref": "DeviceHealthCheck",
      "inputs": {
        "RestrictedMode": false
      }
    },
    {
      "nodeId": "CameraVerify",
      "ref": "CameraVerify",
      "inputs": {
        "MinExpectedCount": "1"
      }
    }
  ]
}