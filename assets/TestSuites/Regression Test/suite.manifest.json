{
  "schemaVersion": "1.5.0",
  "id": "RegressionTest",
  "name": "Regression Test",
  "description": "New test suite",
  "version": "1.0.0",
  "tags": [
    "regression",
    "retest"
  ],
  "controls": {
    "repeat": 2,
    "maxParallel": 1,
    "continueOnFailure": true,
    "retryOnError": 0,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "CameraVerify",
      "ref": "CameraVerify",
      "inputs": {
        "MinExpectedCount": "1"
      }
    },
    {
      "nodeId": "WindowsUpdateHistoryVerify",
      "ref": "WindowsUpdateHistoryVerify",
      "inputs": {
        "MinExpectedCount": "1"
      }
    },
    {
      "nodeId": "TimezoneCheck",
      "ref": "TimezoneCheck",
      "inputs": {
        "ExpectedUtcOffset": "\u002B08:00"
      }
    },
    {
      "nodeId": "EventViewerCheck",
      "ref": "EventViewerCheck",
      "inputs": {
        "EventCount": "100",
        "ProhibitedLevel": "Error",
        "LogName": "System"
      }
    },
    {
      "nodeId": "NetworkPingConnectivity",
      "ref": "NetworkPingConnectivity",
      "inputs": {
        "TargetAddress": "www.microsoft.com",
        "MaxReplyTimeMs": "0"
      }
    }
  ]
}