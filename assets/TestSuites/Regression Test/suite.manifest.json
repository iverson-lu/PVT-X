{
  "schemaVersion": "1.5.0",
  "id": "RegressionTest",
  "name": "Regression Test",
  "description": "New test suite",
  "version": "1.0.0",
  "controls": {
    "repeat": 1,
    "maxParallel": 1,
    "continueOnFailure": false,
    "retryOnError": 0,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "NetworkPing",
      "ref": "NetworkPing",
      "inputs": {
        "TargetAddress": "www.google.com"
      }
    },
    {
      "nodeId": "CameraVerify",
      "ref": "CameraVerify",
      "inputs": {
        "MinExpectedCount": "1"
      }
    },
    {
      "nodeId": "WindowsUpdateHistoryVerify",
      "ref": "WindowsUpdateHistoryVerify",
      "inputs": {
        "MinExpectedCount": "1"
      }
    }
  ]
}