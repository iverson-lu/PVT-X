{
  "schemaVersion": "1.5.0",
  "id": "RepeatTest",
  "name": "Repeat Test",
  "description": "For repeatly testing",
  "version": "1.0.0",
  "controls": {
    "repeat": 2,
    "maxParallel": 1,
    "continueOnFailure": true,
    "retryOnError": 1,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "hw.devices.health_check",
      "ref": "Device Health Check",
      "inputs": {
        "RestrictedMode": false
      }
    },
    {
      "nodeId": "sw.os.windows.timezone_verify",
      "ref": "Timezone Verification",
      "inputs": {
        "ExpectedUtcOffset": "\u002B08:00"
      }
    }
  ]
}