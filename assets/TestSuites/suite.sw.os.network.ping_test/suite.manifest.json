{
  "schemaVersion": "1.5.0",
  "id": "suite.sw.os.network.ping_test",
  "name": "Network Test",
  "description": "Network test with ping multiple websites",
  "version": "1.0.0",
  "tags": [
    "network"
  ],
  "controls": {
    "repeat": 1,
    "maxParallel": 1,
    "continueOnFailure": false,
    "retryOnError": 0,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "sw.network.ping_connectivity",
      "ref": "Network Ping Connectivity - MS",
      "inputs": {
        "TargetAddress": "www.microsoft.com",
        "MaxReplyTimeMs": "0"
      }
    },
    {
      "nodeId": "sw.network.ping_connectivity_1",
      "ref": "Network Ping Connectivity - Baidu",
      "inputs": {
        "TargetAddress": "www.baidu.com",
        "MaxReplyTimeMs": "0"
      }
    },
    {
      "nodeId": "sw.network.ping_connectivity_2",
      "ref": "Network Ping Connectivity - Google",
      "inputs": {
        "TargetAddress": "www.google.com",
        "MaxReplyTimeMs": "0"
      }
    }
  ]
}