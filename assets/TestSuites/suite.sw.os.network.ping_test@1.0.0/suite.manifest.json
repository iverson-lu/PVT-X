{
  "schemaVersion": "1.5.0",
  "id": "suite.sw.os.network.ping_test",
  "name": "Network Test",
  "description": "Network test with ping multiple websites",
  "version": "1.0.0",
  "tags": [
    "network"
  ],
  "controls": {
    "repeat": 1,
    "maxParallel": 1,
    "continueOnFailure": false,
    "retryOnError": 1,
    "timeoutPolicy": "AbortOnTimeout"
  },
  "testCases": [
    {
      "nodeId": "case.network.core.connectivity.ping@1.0.0",
      "ref": "Network Ping Connectivity - MS",
      "inputs": {
        "TargetAddress": "www.microsoft.com",
        "MaxReplyTimeMs": "0"
      }
    },
    {
      "nodeId": "case.os.windows.core.event_log_check@1.0.0",
      "ref": "Event Viewer Level Check",
      "inputs": {
        "EventCount": "100",
        "ProhibitedLevel": "Error",
        "LogName": "System"
      }
    },
    {
      "nodeId": "case.network.core.connectivity.ping@1.0.0_1",
      "ref": "Network Ping Connectivity - Baidu",
      "inputs": {
        "TargetAddress": "www.baidu.com",
        "MaxReplyTimeMs": "0"
      }
    }
  ]
}